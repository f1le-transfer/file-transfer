{
  "name": "file-transfer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "server": "node build-server/index.js",
    "transpile": "babel ./src/server --out-dir build-server",
    "clean": "rimraf ./build-server",
    "build": "sh bin/run-all.sh clean transpile",
    "dev": "NODE_ENV=development sh bin/run-all.sh build server",
    "prod": "NODE_ENV=production sh bin/run-all.sh build server",
    "start": "npm run prod",
    "watch:dev": "nodemon"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "author": "loveyousomuch554@gmail.com",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongodb": "^3.6.3",
    "multer": "^1.4.2",
    "regenerator-runtime": "^0.13.7"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2"
  }
}
